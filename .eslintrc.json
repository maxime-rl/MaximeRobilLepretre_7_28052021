{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "standard",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser":"babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "double", "avoid-escape"],
    "semi-spacing": 2,
    "semi": [2, "always"],
    "indent": 2
  }
}